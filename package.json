{
  "name": "whop-clipping-agency",
  "version": "0.1.0",
  "description": "Automated clipping agency management on Whop platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "setup": "tsx src/scripts/setup-whop.ts",
    "test:scrapcreators": "npx tsx scripts/test-scrapcreators.ts",
    "test:drive": "npx tsx scripts/test-drive.ts",
    "test:workflow": "npx tsx scripts/test-workflow.ts",
    "test:api": "npx tsx scripts/test-api.ts",
    "test:youtube": "npx tsx scripts/test-youtube.ts",
    "import:youtube": "npx tsx scripts/import-youtube-clip.ts"
  },
  "keywords": [
    "whop",
    "clipping",
    "agency",
    "automation"
  ],
  "author": "BHT Labs",
  "license": "UNLICENSED",
  "dependencies": {
    "@whop/sdk": "^0.0.23",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^144.0.0",
    "node-fetch": "^3.3.2",
    "uuid": "^11.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
